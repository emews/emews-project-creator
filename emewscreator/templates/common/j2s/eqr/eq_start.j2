(void o) start (int ME_rank) {
    string me_script = "%s/%s" % (emews_root, "{{cookiecutter.me_script}}");
    location me_loc = locationFromRank(ME_rank);
    EQR_init_script(me_loc, me_script) =>
    EQR_get(me_loc) =>
    EQR_put(me_loc, me_config_file) =>
      loop(me_loc) => {
        EQR_stop(me_loc) =>
        EQR_delete_R(me_loc);
        o = propagate();
    }
}